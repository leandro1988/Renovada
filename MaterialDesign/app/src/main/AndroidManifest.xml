<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="info.androidhive.materialdesign">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:icon="@drawable/renova"
        android:label="@string/app_name"
        android:theme="@style/MyMaterialTheme">
        <activity android:name="info.androidhive.renovada.activity.SplashActivity" >

        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>

        </activity>

        <activity
            android:name="info.androidhive.renovada.activity.MainActivity"
            android:label="@string/app_name" />

        <activity
            android:name="info.androidhive.renovada.activity.LoginActivity"
            android:label="@string/app_name" />

        <activity
            android:name="info.androidhive.renovada.activity.MembroActivity"
            android:label="@string/app_name" />


        <!--

 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

</manifest>